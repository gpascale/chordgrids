n\n        _getOwnerId: function(owner) {\n            if (!owner) {\n                return '__default__';\n            }\n\n            if (owner && (owner.cid != null)) {\n                return owner.cid;\n            }\n            else {\n                return owner;\n            }\n        }\n    });\n})();\n\n</script>\n</apex:component>",
